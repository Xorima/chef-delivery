{
  "problemMatcher": [
    {
      "owner": "rspec",
      "pattern": [
        {
          "regexp": ".*\\)\\s(.*)\\s+(\\w+\\/\\w+)\\:\\s+(.*\\s*expected.*\\s+got.*)\\s+\\#\\s+\\.\\/(.*)\\:(\\d+)\\:.*",
          "file": 4,
          "line": 5,
          "code": 2,
          "message": 3
        }
      ]
    }
  ]
}
